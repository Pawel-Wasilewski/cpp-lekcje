#include <iostream>
#include <set>
#include <iterator>
using namespace std;

void tworzenie(set<int>& sorcik, int ilosc){
    
    for(int i=0; i<ilosc; i++){
        

    int a = rand() % 1000;
    cout << a << endl;
    sorcik.insert(a);
    
    }
}

void szukanie(const set<int>& sorcik, int lewo, int prawo){
    
    int srodek = (lewo + prawo)/2;
    
    auto it = next(sorcik.begin(), srodek);
    
    cout << *it;
    
}

int main(){
    srand((unsigned)time(NULL));
    set<int> sorcik;
    int ilosc;
    int szukadlo;
    cout << "Podaj ilosc liczb: ";
    cin >> ilosc;
    
    tworzenie(sorcik, ilosc);
    
    cout << "Jaka liczba wariacie? ";
    cin >> szukadlo;
    
    int lewo = 0;
    int prawo = sorcik.size() - 1;
    
    szukanie(sorcik, lewo, prawo);
    
    
    return 0;
    
}
